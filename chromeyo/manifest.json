{
  "name": "The Payroll",
  "description": "Custom Chrome Extension for WallStreet Finish Class",
  "version": "1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "third-party/jquery-1.12.4.js",
        "detectclicks.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "bookmarks",
    "storage",
    "scripting",
    "unlimitedStorage"
  ],
  "action": {
    "default_title": "The Payroll",
    "default_icon": "icon.png",
    "default_popup": "popup2.html"
  },
  "key": ""
}
